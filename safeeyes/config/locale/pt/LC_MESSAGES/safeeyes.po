msgstr "Pausa curta em %s segundos"

# plugin/notification
msgid "Ready for a long break in %s seconds"
msgstr "Pausa longa em %s segundos"

# plugin/screensaver
msgid "Screensaver"
msgstr "Proteção de ecrã"

