# plugin/mediacontrol
msgid "Media Control"
msgstr "Multivides kontroles"

# plugin/mediacontrol
msgid "Pause media players from the break screen"
msgstr "Pauzēt multivides atskaņotājus no pārtraukuma ekrāna"

# plugin/mediacontrol
